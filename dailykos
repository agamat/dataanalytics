dailykos <- read.csv("dailykos.csv")
distance <- dist(dailykos, method="euclidean")
kosclust <- hclust(distance, method="ward.D")
plot(kosclust)
clustgroup <- cutree(kosclust, k=7)
clust1 <- subset(dailykos, clustgroup==1)
clust2 <- subset(dailykos, clustgroup==2)
clust3 <- subset(dailykos, clustgroup==3)
clust4 <- subset(dailykos, clustgroup==4)
clust5 <- subset(dailykos, clustgroup==5)
clust6 <- subset(dailykos, clustgroup==6)
clust7 <- subset(dailykos, clustgroup==7)
table(clsutgroup)
table(clustgroup)
tail(sort(colMeans(clust1)))
tail(sort(colMeans(clust2)))
tail(sort(colMeans(clust3)))
tail(sort(colMeans(clust4)))
tail(sort(colMeans(clust5)))
tail(sort(colMeans(clust6)))
tail(sort(colMeans(clust7)))
set.seed(1000)
kmeans <- kmeans(dailykos, centers=7)
kmeans1 <- subset(dailykos, kmeans$cluster ==1)
kmeans2 <- subset(dailykos, kmeans$cluster ==2)
kmeans3 <- subset(dailykos, kmeans$cluster ==3)
kmeans4 <- subset(dailykos, kmeans$cluster ==4)
kmeans5 <- subset(dailykos, kmeans$cluster ==5)
kmeans6 <- subset(dailykos, kmeans$cluster ==6)
kmeans7 <- subset(dailykos, kmeans$cluster ==7)
nrow(kmeans3)
table(kmeans)
table(kmeans1)
nrow(kmeans1)
nrow(kmeans2)
nrow(kmeans3)
nrow(kmeans4)
nrow(kmeans5)
nrow(kmeans6)
nrow(kmeans7)
tail(sort(colMeans(kmeans1)))
tail(sort(colMeans(kmeans2)))
tail(sort(colMeans(kmeans3)))
tail(sort(colMeans(kmeans4)))
tail(sort(colMeans(kmeans5)))
tail(sort(colMeans(kmeans6)))
tail(sort(colMeans(kmeans7)))
table(clustgroup, kmeans$cluster)
