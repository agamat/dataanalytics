airlines <- read.csv("AirlinesCluster.csv")
summary(airlines)
library(caret)
preproc <- preProcess(airlines)
airlinesnorm <- predict(preproc, airlines)
summary(airlinesnorm)
distancenorm <- dist(airlinesnorm, method="euclidean")
normclust <- hclust(distancenorm, method="ward.D")
plot(normclust)
clustgroupnorm <- cutree(normclust, k=5)
clustgroup1 <- subset(airlinesnorm, clustgroupnorm ==1)
nrow(clustgroup1)
tapply(airlines$Balance, clustgroupnorm, mean)
tapply(airlines$QualMiles, clustgroupnorm, mean)
tapply(airlines$BonusMiles, clustgroupnorm, mean)
tapply(airlines$BonusTrans, clustgroupnorm, mean)
tapply(airlines$FlightMiles, clustgroupnorm, mean)
tapply(airlines$FlightTrans, clustgroupnorm, mean)
tapply(airlines$DaysSinceEnroll, clustgroupnorm, mean)
set.seed(88)
kmeansclust <- kmeans(airlinesnorm, centers=5, iter.max=1000)
table(kmeansclust$cluster
)
kmeansclust$cluster
plot(kmeansclust$cluster)
